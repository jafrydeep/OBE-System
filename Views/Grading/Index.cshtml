@model Grading

<div>
    <div class="pt-2">
        @Html.Partial("Notifications/_Notification")
    </div>
    <div class="row">
        <h2 class="mt-4 mb-1 font-weight-light"><i class="fas fa-money-check p-1"></i>&nbsp; Grading Manager</h2>
        <hr />

    </div>
    <hr class="c-hr-gray" />

    <div class="pt-2">
        @Html.Partial("Partial/_Cards")
    </div>
    <hr class="c-hr-gray" />
    <ul class="nav nav-tabs mb-1" id="myTab" role="tablist">

        <li class="nav-item">
            <a class="nav-link active" id="users-tab" data-toggle="tab" href="#user" role="tab" aria-controls="user" aria-selected="true">Grading System</a>
        </li>
        <li class="nav-item">
            <a class="nav-link" id="vets-tab" data-toggle="tab" href="#vet" role="tab" aria-controls="vet" aria-selected="false">Gradings</a>
        </li>
    </ul>
    <div class="tab-content" id="myTabContent">
        <div class="tab-pane fade show active" id="user" role="tabpanel" aria-labelledby="user-tab">
            @Html.Partial("Partial/GradingSystems", Model.GradingSystems)
        </div>
        <div class="tab-pane fade" id="vet" role="tabpanel" aria-labelledby="vet-tab">
            @Html.Partial("Partial/GradingSystems", Model.GradingSystems)
        </div>
    </div>
</div>

<!--Profile Modal-->
<div class="modal fade" id="view-gradingsyatem-modal" tabindex="-1" role="dialog" aria-labelledby="modal-default" aria-hidden="true">
    <div class="modal-dialog modal-lg modal-dialog-centered modal-" role="document" id="viewgradingsyatemmodal">

    </div>
</div>
