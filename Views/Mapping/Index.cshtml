@model OBETools.Models.View_Model.Mapping

<div>
    <div >
        @Html.Partial("Notifications/_Notification")
    </div>
    <div class="row">
        <h2 class="mt-4 mb-1 font-weight-light"><i class="fas fa-map-signs p-1"></i>&nbsp; OBE Objects Mapping</h2>
        <hr />

    </div>
    <hr class="c-hr-gray" />
    <!-------------------TAB------------------>
    <ul class="nav nav-tabs mb-1" id="myTab" role="tablist">

        <li class="nav-item">
            <a class="nav-link active" id="users-tab" data-toggle="tab" href="#user" role="tab" aria-controls="user" aria-selected="true">Mission to PEO</a>
        </li>
        <li class="nav-item">
            <a class="nav-link" id="vets-tab" data-toggle="tab" href="#vet" role="tab" aria-controls="vet" aria-selected="false">PLO to PEO</a>
        </li>

        <li class="nav-item">
            <a class="nav-link" id="admins-tab" data-toggle="tab" href="#admin" role="tab" aria-controls="admin" aria-selected="false">PLO to CLO</a>
        </li>
    </ul>

    <div class="tab-content" id="myTabContent">

        <div class="tab-pane fade show active" id="user" role="tabpanel" aria-labelledby="user-tab">
            @Html.Partial("Partial/MissionToPEO", Model.MissionToPEOs)
        </div>
        <div class="tab-pane fade" id="vet" role="tabpanel" aria-labelledby="vet-tab">
            @Html.Partial("Partial/PLOToPEO", Model.PLOToPEOs)
        </div>

        <div class="tab-pane fade " id="admin" role="tabpanel" aria-labelledby="admin-tab">
            @Html.Partial("Partial/CLOToPLO", Model.CLOToPLOs)
        </div>
    </div>
    <!-------------------TAB------------------>

</div>


<!--Mission to PEO Modal-->
<div class="modal fade" id="view-missiontopeo-modal" tabindex="-1" role="dialog" aria-labelledby="modal-default" aria-hidden="true">
    <div class="modal-dialog modal-lg modal-dialog-centered modal-" role="document" id="viewmissiontopeomodal">

    </div>
</div>

<!--PLO to PEO Modal-->
<div class="modal fade" id="view-plotopeo-modal" tabindex="-1" role="dialog" aria-labelledby="modal-default" aria-hidden="true">
    <div class="modal-dialog modal-lg modal-dialog-centered modal-" role="document" id="viewplotopeomodal">

    </div>
</div>


<!--CLO To PLO Modal-->
<div class="modal fade" id="view-clotoplo-modal" tabindex="-1" role="dialog" aria-labelledby="modal-default" aria-hidden="true">
    <div class="modal-dialog modal-lg modal-dialog-centered modal-" role="document" id="viewclotoplomodal">

    </div>
</div>
