@model Users
    <div>
        <div class="pt-2">
            @Html.Partial("Notifications/_Notification")
        </div>

        <div class="row">
            <h2 class="mt-4 mb-1 font-weight-light"><i class="fas fa-users p-1"></i>&nbsp; Users</h2>
        </div>
        <hr class="c-hr-gray" />

        <div class="pt-2">
            @Html.Partial("Partial/_Cards")
        </div>

        <hr class="c-hr-gray" />
        <ul class="nav nav-tabs mb-1" id="myTab" role="tablist">
            <li class="nav-item">
                <a class="nav-link active" id="users-tab" data-toggle="tab" href="#user" role="tab" aria-controls="user" aria-selected="true">Students</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" id="vets-tab" data-toggle="tab" href="#vet" role="tab" aria-controls="vet" aria-selected="false">Staffs</a>
            </li>
        </ul>

        <div class="tab-content" id="myTabContent">
            <div class="tab-pane fade show active" id="user" role="tabpanel" aria-labelledby="user-tab">
                @Html.Partial("Partial/Students", Model.Students)
            </div>
            <div class="tab-pane fade" id="vet" role="tabpanel" aria-labelledby="vet-tab">
                @Html.Partial("Partial/Staffs", Model.Staffs)
            </div>
        </div>
    </div>
